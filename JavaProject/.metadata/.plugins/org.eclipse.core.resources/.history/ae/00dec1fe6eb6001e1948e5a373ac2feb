		super(title);

		cp = this.getContentPane();

		this.setBounds(500, 500, 500, 400);
		cp.setBackground(new Color(255, 255, 200));



		this.setVisible(true);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
