package day0111;

class MyCar1{
	String carName;
	int carPrice;
	String carColor;
}

public class CallByReference_13 {
	
	public static void dataIn(MyCar1 car1) {
		car1.carName = "미니쿠퍼";
		car1.carPrice = 40000000;
		car1.carColor = "초록색";
	}
	
	public static void dataOut() {
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
