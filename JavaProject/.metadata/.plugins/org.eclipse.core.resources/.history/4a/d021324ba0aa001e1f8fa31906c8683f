package day0104;

public class DataType_06 {

	// boolean flag
	public static void main(String[] args) {
		// 기본자료형 8개
		boolean flag = false; // 논리형 true, false, boolean flag;(x)
		
		// 문자형
		// 반드시 하나의 문자 16byte 유니코드 값 표현
		// 2byte 문자 하나
		// 기본적으로 정수형, 아스키코드로 변경 가능
		// ''로 표현
		char ch = '가';

	}

}
