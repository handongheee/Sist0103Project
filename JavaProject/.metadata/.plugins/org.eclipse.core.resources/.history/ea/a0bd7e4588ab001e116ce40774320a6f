package day0105;

import java.util.Scanner;

public class IfOper_12 {

	public static void main(String[] args) {
		// 삼항(조건)연산자
		// 형식조건식 ? 참:거짓
		Scanner sc = new Scanner(System.in);
		int  x, y;
		
		System.out.println("**두 개의 숫자를 입력하세요.**");
		x = sc.nextInt();
		y = sc.nextInt();
		
		System.out.println("입력한 두 수는 "+x+", "+y);
		

	}

}
