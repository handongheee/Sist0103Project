package day0112;

class Emart{
	private String sangpum;
	private int su;
	
	// 디폴트 생성자
	public Emart() {
		//sangpum="딸기";
		//su=10;
		this("딸기", 10);
	}
	
	// 명시적 생성자
	public Emart(String sangpum, int su) {
		this.sangpum=sangpum;
		this.su=su;
	}
	
	
}

