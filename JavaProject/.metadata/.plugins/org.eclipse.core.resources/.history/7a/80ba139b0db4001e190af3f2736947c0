package day0116;

class OuterObj{
	
	public void disp() {
		System.out.println("외부클래스의 disp 메서드");
	}
	
	
	// OuterObj 클래스에서만 기능하는 클래스
	class innerObj{
		
		public void write() {
			System.out.println("내부클래스의 write 메서드");
			
		}
		
	}
	
	
}

//////////////////////////////
public class InnerClass_04 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
