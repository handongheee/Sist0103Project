package day0111;

class Apple{
	public Apple() {
		// TODO Auto-generated constructor stub
		System.out.println("Apple_디폴트 생성자"); // 굳이 선언 안해도 존재함, 출력하기 위해 따로 작성
	}
	
}

///////////////////////////////////
class Banana{
	public Banana(String str) {
		// TODO Auto-generated constructor stub
		System.out.println("생성자 호출: "+str);
		
	}

}

