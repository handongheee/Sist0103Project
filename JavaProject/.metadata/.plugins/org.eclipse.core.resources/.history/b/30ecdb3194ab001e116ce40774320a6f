package day0105;

public class SwitchTest_17 {

	public static void main(String[] args) {
		// Switch문 예제 #01
		int a=2;
		
		// break가 없을 경우 끝까지 실행
		switch(a)
		{
			case 1: 
				System.out.println("a is 1");
			case 2: 
				System.out.println("a is 2");
			case 3: 
				System.out.println("a is 3");
			default:
				System.out.println("a is 입력값 없음");
		} 

	}

}
