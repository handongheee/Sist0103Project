package day0108;

public class QuizFor_12 {

	public static void main(String[] args) {
		// 1. 1 2 4 5 7 8 10 (continue 활용)
		for(int i=1; i<=10; i++) {
			if(i%3==0)
				continue;
			System.out.print(i+" ");
		}
		System.out.println();
		
		// 2. 홀수값:1 홀수값:3 홀수값:5 홀수값:7
		for(int a=1; a<=10; a++) {
			if(a%2==0)
				continue;
			System.out.print("홀수값:"+a+" ");
		}

	}

}
