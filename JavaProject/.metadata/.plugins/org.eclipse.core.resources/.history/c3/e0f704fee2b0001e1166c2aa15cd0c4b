package day0112;

class Food{
	private String foodName;
	private int fPrice;
	
	static int no;
	
	// 명시적 생성자(2개)
	public void setFood(String food, int price) {
		this.foodName=food;
		this.fPrice=price;
	}
	
	// 출력문
	public void writeFood() {
		no++;
		System.out.println("[메뉴_"+no+"]");
		System.out.println("음식명: "+foodName);
		System.out.println("가격: "+fPrice);
		System.out.println("---------------------------");
	}
}

