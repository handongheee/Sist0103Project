package day0104;

public class DataType_09 {

	public static void main(String[] args) {
		// 묵시적 형변환은 자동으로 결정되는 타입
		// String+int=String
		// double+int=double
		// long+short=long
		// char+int=int
		
		char a = 'A'; // 65
		// char는 내부적으로 아스키코드 값으로 인식
		
		System.out.println(a+32);
		System.out.println("Happy"+5+6);
		System.out.println("Happy"+(5+6)); //괄호 안부터 먼저 더해진다

	}

}
