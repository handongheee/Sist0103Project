package day0110;

class Numb{
	int num; // 0
	
	// 디폴트 생성자 (인자값이 생기면 다시 만들어야 됨)
	public Numb() {
		System.out.println("디폴트 생성자 호출");
	
	}
	
	// 인자있는 생성자
	public Numb(int num) {
		this.num=num;
		System.out.println("인자 있는 생성자 호출");
		
	}
	
	
	
}

////////////////////////////////////////
public class ConstNum_11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Numb n1 = new Numb();
		
		Numb n2 = new Numb(10);

	}

}
