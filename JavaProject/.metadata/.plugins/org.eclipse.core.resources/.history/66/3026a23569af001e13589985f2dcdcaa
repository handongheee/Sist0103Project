package day0110;

public class CardTest_08 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Card.width = 10;
		Card.height = 20;
		
		// 클래스가 로딩될 때 변수명도 바로 로딩됨
		System.out.println(Card.width);
		System.out.println(Card.height);
		
		// 인스턴스 변수 생성
		Card c1 = new Card();
		System.out.println(c1.kind);
	}

}
