package day0110;

class Ipgo{
	private String sangName;
	private int Price;
	
	// set 메서드
	public void setName(String sangName) {
		this.sangName = sangName;
	}
	
	public void setPrice(int Price) {
		this.Price = Price;
	}
	
	// get 메서드
	public String getName() {
		return sangName;
	}
	
	public int getPrice() {
		return Price;
	}
	
}

