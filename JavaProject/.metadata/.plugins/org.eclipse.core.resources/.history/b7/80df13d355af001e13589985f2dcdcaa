package day0110;

public class ArraysChange_03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a=10, b=20;
		System.out.println("a="+a+", b="+b);
		
		int temp=a;
		a=b;
		b=temp;
		
		System.out.println("tmp a="+a+", b="+b);
		
		int [] n = {4, 6, 1};
		
		for(int s: n) {
			System.out.println(s);
		}

	}

}
